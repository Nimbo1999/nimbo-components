{
    "presets": [
        "@babel/preset-react",
        "@babel/preset-env",
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-typescript",
            {
                "isTSX": true
            }
        ],
        "@babel/plugin-transform-spread",
        "@babel/plugin-transform-template-literals",
        "@babel/plugin-proposal-class-properties"
    ]
}